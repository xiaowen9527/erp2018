                border-radius 4px
                &.button_btn
                    border .1vh solid #409EFF
                    font-weight bold
                    color #409EFF

                     
//子页面页脚和分页
.pageBox
    width 87.5vw
    min-width 1300px
    height 4vh
    border-radius 2px
    background #f2f2f2
    border: .1vh solid #cccccc
    border-top: .1vh solid #cccccc
    position: fixed
    bottom 3.5vh
    &.collapse
        .el-pagination
            right: 0
    li
        float left
        height 3vh !important
        span
            line-height 4vh !important
            padding 0 .5vh
            float left
            display block
            font-size 1.5vh
            color #444444
    .el-pagination
        width: 30%    
        position: absolute
        top: .4vh  
        right: 12%
        background: #f2f2f2
        
    .pageData
        height 4vh
        float left
        li,span
            height 4vh !important
            line-height: 4vh !important
            font-size: 1.2vh
        .el-pager li
            font-size: 1.2vh
            line-height: 2.8vh
            height 2.8vh !important
    .el-pagination.is-background .el-pager li:not(.disabled).active
        height 2.8vh !important
        line-height: 2.8vh
        font-size: 1.5vh
    .el-pagination.is-background .btn-next, .el-pagination.is-background .btn-prev, .el-pagination.is-background .el-pager li
        line-height: 2.8vh
        font-size: 1.5vh
    .el-pagination button
        line-height: 2.8vh
        font-size: 1.5vh  
        height 2.8vh
    .el-pagination button
        height 2.8vh !important
    .el-pager .more::before
        line-height: 3vh !important     
    .el-pagination span:not([class*=suffix])
        display: inline-block;
        font-size: 1.5vh !important 
        height: 2.8vh !important 
        line-height: 2.8vh !important 
    .el-pagination__editor.el-input .el-input__inner
        height 2.8vh !important     
          
