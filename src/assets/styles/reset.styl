                .save
                    padding 0 2vh
                    height 3.5vh
                    line-height 3.5vh
                    border .1vh solid #d9d9d9
                    background #ffffff
                    margin-top 2px
                    font-size 1.4vh
                    display block
                    float left
                    text-align center
                    text-decoration underline
                    margin-left 30%
                    cursor pointer
                    border-radius 4px
                    &.button_btn
                        border .1vh solid #409EFF
                        font-weight bold
                        color #409EFF
                button.gui-btn
                    border .1vh solid #409EFF !important
                    font-weight bold
                    color #409EFF

        .secondForm
            border .1vh solid #d2d2d2
            display: block
            width 100%
            height 100%
            min-width 900px
            border-radius .5vh  
            margin-top: .5vh
            li
                width 25%
                height 3.5vh
                line-height 3.5vh
                float left
                display block
                margin 1vh 0
                &.gui
                    width 35%
                    label
                        width 25%
                        white-space: normal
                    input
                        width 40%
                        margin-right .5vh
                        &.guiNum
                            width 15%
                            text-align center
                            padding: 0
                    button
                        width 50px
                        height 3.5vh
                        border .1vh solid #d2d2d2
                        background none
                        line-height 3.5vh
                        text-align center
                        border-radius .5vh
                        cursor pointer
                label
                    width 30%
                    display block
                    float left
                    text-align center
                    word-wrap hidden
                    &.name
                        font-size 1.4vh
                input
                    line-height 3.5vh
                    height 3.5vh
                    float left
                    width 70%
                    padding-left: 1vh
                    &.half
                        width 30%
                        margin-right .5vh
            .save
                padding 0 2vh
                height 3.5vh
                line-height 3.5vh
                border .1vh solid #d9d9d9
                background #ffffff
                margin-top 2px
                font-size 1.4vh
                display block
                float left
                text-align center
                text-decoration underline
                margin-left 10%
