    width 100%
    height 75.5vh
    position relative
    overflow-x hidden   
    .menu_box
        height 100%
        position absolute
        top 0
        left 0
        background #f2f2f2
        overflow auto
        border: solid .1vh #e6e6e6    

        ul
            height 71%  
            // overflow-x hidden
            &.el-menu
                border: none     
    .set_info
