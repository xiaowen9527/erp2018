* 
  -webkit-box-sizing border-box
  -moz-box-sizing border-box
  box-sizing border-box
*:before,
*:after 
  -webkit-box-sizing border-box
  -moz-box-sizing border-box
  box-sizing border-box
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, button, textarea, p, blockquote, th, td 
  margin 0
  padding 0
// body
//     overflow hidden
label
    white-space nowrap !important
body 
  background #fff
  color #555
  font-size 1.6vh
  font-family "Arial", "Microsoft YaHei", "黑体", "宋体", sans-serif
td, th, caption 
  font-size 1.6vh
h1, h2, h3, h4, h5, h6 
  font-weight normal
  font-size 100%
address, caption, cite, code, dfn, em, strong, th, var 
  font-style normal
  font-weight normal
a 
  color #555
  text-decoration none
  &:hover 
    text-decoration underline
img 
  border none
  vertical-align middle
ol, ul, li 
  list-style none
input, textarea, select, button 
  font 1.6vh "Arial", "Microsoft YaHei", "黑体", "宋体", sans-serif
  outline none
  cursor pointer
  word-break normal
  white-space: nowrap!important;
  text-overflow: ellipsis!important;
  overflow: hidden!important;
  cursor pointer  
input::-webkit-outer-spin-button,input::-webkit-inner-spin-button 
    -webkit-appearance: none !important;       
input,select
    border: .1vh solid #d2d2d2
table 
  border-collapse collapse
html 
  overflow hidden
.clearfix:before,
.clearfix:after 
  content " "
  display block
  height 0
  clear both
  visibility hidden
.clearfix 
  *zoom 1
.order_table
    border-width: .1vh !important
/*公共类*/
.fl 
  float left
.fr 
  float right
.al 
  text-align left
.ac 
  text-align center
.ar 
  text-align right
.hide 
  display none

.el-submenu__icon-arrow
    position: static !important
    margin-top: 0
.el-submenu .el-menu-item
    min-width auto  !important  

/* 子页面公共样式 */
.container 
  padding-left: .5vh
  position absolute
  top 8.5vh
  left 12vw
  width 88vw
  transition left .3s ease-in-out
.container_collapse 
  left 0 !important
  width 100%
/* 子页面标题 */
.page_title 
  width 100%
  line-height 4vh
  text-align center
  color #444444
  font-weight bold
  font-size 2.5vh
/* 子页面头部按钮条 */
.btn-box
    width 100%
    min-width 1400px
    height 4.5vh
    border .2vh solid #d9d9d9
    border-radius 2px
    button,a.export_btn
        padding 0 2vh
        height 3.5vh
        line-height 3.5vh
        border .1vh solid #d9d9d9
        background #ffffff
        margin .3vh .3vh
        font-size 1.4vh
        display block
        float left
        text-align center
        text-decoration underline
        cursor pointer
        border-radius 4px
        &.button_btn
            border .1vh solid #409EFF
            font-weight bold
            color #409EFF
    .doSearch
        width:15%
        height 3.5vh
        margin-top: .3vh
        border-radius: 2px
        border: .1vh solid #d2d2d2
        padding-left: 1vh
        float left
        cursor: pointer
    .btn_right
        float right
        margin-right 3vh            
//子菜单主体
