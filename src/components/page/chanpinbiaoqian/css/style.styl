.btn-box
    min-width 500px
.set_box    
    min-width 1280px 
    .menu_box
        width 15%
        position: relative
        .searchYear
            position: absolute
            top 0
            width: 100%
        .el-menu
            position: absolute
            top: 4vh
            width: 100%
    .set_info
        width 85%
        .firstForm
            padding: 1vh
        .main
            height 50vh
            .left
                float left
                width: 42vw 
                ul
                    width: 46vw
                    li
                        height 3vh
                        label
                            width: 100px
                            display: block
                            height 3vh
                            line-height: 3vh
                            text-align: center
                            float left
                        .el-checkbox
                            float left
                            margin-left: .5vh
                            width: auto
                            padding: 0 1vh
            .right
                float left
                width: 30vw
                margin: 0 1vh
                max-height 50vh
                img 
                    display: block;
                    max-height: 48vh
                    width 100%
                    margin-top: 1vh
                    cursor pointer
.set_box
    height 79.5vh                    
.set_box .set_info .main .left ul
    width: 100%
    min-width 300px
.order_table
    width 100%
    margin 5px auto 0
    height 26vh
    border 0.1vh solid #d9d9d9
    .el-button
        padding 8px 2vh
        margin-top 0.4vh
        border 0.1vh solid #409eff
        color #409eff
        &.btn
            border 0.1vh solid #d2d2d2
            color #d2d2d2                        